Helpers
	any_charactar = [0 .. 0xffff];
	letter = [['a' .. 'z'] + ['A' .. 'Z']];
	digit = ['0' .. '9'];
	a = 'a' | 'A';
	b =	'b' | 'B';
	d =	'd' | 'D';
	e =	'e' | 'E';
	g =	'g' | 'g';
	i =	'i' | 'I';
	l =	'l' | 'L';
	m =	'm' | 'M';
	n =	'n' | 'N';
	o =	'o' | 'O';
	p =	'p' | 'P';
	r =	'r' | 'R';
	t =	't' | 'T';
	v =	'v' | 'V';
	w =	'w' | 'W';

Tokens
	comment = ('⁄⁄' any_character* '\n') | ('/*' + any_character* + '*/');
	blank = (' ' | 10 | 13 | 9)*;
	identifier = (letter | '_')* + (letter | '_' | digit)*;
	
	int = digit+;
	float = digit+ + '.' + digit+;
	bool = 'true' 

Ignored Tokens
	comment;
	blank;